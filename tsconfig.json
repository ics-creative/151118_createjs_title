{
  "compilerOptions": {
    "strictNullChecks": true,
    "noUnusedLocals": false,
    "noImplicitThis": false,
    "alwaysStrict": false,
    "sourceMap": true,
    "noImplicitAny": true,
    "lib": [
      "dom",
      "es2018"
    ],
    "module": "es2015",
    "target": "es5",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules"
  ]
}
